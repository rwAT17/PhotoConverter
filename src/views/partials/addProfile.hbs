	<div class='wrapper'>
	
	    

	
     <div class="add-profile">
		<form class="add-profile__form" method="POST" action="/addProfile">
		<div class="add-profile__container">		 
			  <div class="add-profile__input"> 
				  <label class="add-profile" for="">Name</label>
		    <input class="dupa"  type="text" name="name" value="Name">
			</div>
			<div class="add-profile__input">
				<label for="">Size</label>
			<input class="dupa" type="text" name="size" value="100">
			</div>
		    	<div class="add-profile__input">
					<label for="">Quality</label>
		         <input class="dupa" type="text" name="quality" value="100">
			</div>
				<div class="add-profile__check">
			<input class="dupa" type="hidden" name="waterMark" value="0"><input type="checkbox" onclick="this.previousSibling.value=1-this.previousSibling.value">
					<label for="">WaterMark</label>
			</div>
				<div class="add-profile__check">	
			<input class="dupa" type="hidden" name="logo" value="0"><input type="checkbox" onclick="this.previousSibling.value=1-this.previousSibling.value">
					<label for="">Logo</label>
			</div>
			
			</div>

		    <input class="dupa" type="submit" value="dupa">
		</form>
 </div>

	<div class="delete-profile">
			<form method="post" action="/addProfile?_method=DELETE">
		     <input type="hidden" name="_method" value="delete">
			 {{#each profilesList}}
		        <div>
			     <input type="checkbox" name="test" value='{{this._id}}'>
			      <p>{{this._id}}</p>
			      <p>{{this.name}}</p>
			      <p>{{this.size}}</p>
			      <p>{{this.quality}}</p>
			      <p>{{this.waterMark}}</p>
			      <p>{{this.logo}}</p>
			    </div>
	         {{/each}}
	        <input type="submit" value="dupa">
	    </form>
	</div>
	
</div>
